{
  "presets": [
    [
      "next/babel",

      {
        "preset-env": {
          "targets": {
            "ie": "11"
          },
          "useBuiltIns": "usage",
          "corejs": 3,
          "exclude": ["proposal-dynamic-import"]
        },
        "@babel/preset-typescript": {},
        "transform-runtime": {
          "plugin-transform-react-jsx": {
            "runtime": "automatic"
          }
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-arrow-functions", "@babel/plugin-proposal-object-rest-spread"]
}

